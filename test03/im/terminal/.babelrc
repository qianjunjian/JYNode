{
    "presets": [
        [
            "@babel/preset-react", {
                "runtime": "automatic"
            }
        ],
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": 3
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": 3,
                "helpers": false
            }
        ]
    ]
}

